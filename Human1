
public class Human {

    private String name;
    protected  int age;

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public int getAge() {
        return age;
    }

    public void setAge(int age) {
        this.age = age;
    }

    
    Human Sasha = new Human();
    Human Pasha = new Human();
              
    public static void main(String[] args) {
            
            Human human = new Human();
            if(human.Pasha.age > human.Sasha.age) {
                System.out.println("Pasha is older than Sasha");
            } else {
                System.out.println("Nope, Sasha is older!");
            }
    }  
}


public class Class1 extends Human{
    
    public void Class1() { 
        Pasha.setAge(18);
        age = Pasha.getAge();
}
    
}


public class Class2 extends Human{

    public void Class2() {
        Sasha.setAge(20);
        age = Sasha.getAge();
    }
}
